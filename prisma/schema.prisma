datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Event {
  id          String   @id @default(uuid())
  userId      String
  name        String
  description String
  location    String
  is_limit    Boolean
  max_people  Int?
  date        DateTime
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
